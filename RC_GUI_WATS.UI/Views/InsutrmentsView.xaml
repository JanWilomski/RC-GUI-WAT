<UserControl x:Class="RC_GUI_WATS.UI.Views.InstrumentsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:RC_GUI_WATS.UI.ViewModels">
    <Grid Margin="10">
        <Button Content="Load Instruments" Command="{Binding LoadCommand}"/>
        <DataGrid ItemsSource="{Binding Instruments}" AutoGenerateColumns="True" Margin="0,40,0,0"/>
    </Grid>
</UserControl>